<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.multithread.task3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.multithread.task.Application.MyApplication">
        <activity android:name="com.multithread.task.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.multithread.task.Loader.LoaderActivity">

        </activity>
        <activity android:name="com.multithread.task.Retain.FragmentActivity"></activity>
        <activity android:name="com.multithread.task.Singleton.SingletonActivity"></activity>
        <activity android:name="com.multithread.task.Application.AppActivity"></activity>
        <activity android:name="com.multithread.task.Service.ActivityService"></activity>

        <service android:name="com.multithread.task.Service.ServiceLoader"></service>
    </application>

</manifest>